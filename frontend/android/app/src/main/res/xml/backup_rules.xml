<?xml version="1.0" encoding="utf-8"?>
<full-backup-content>
    <!-- Include business settings and preferences -->
    <include domain="sharedpref" path="."/>
    <include domain="database" path="business_data.db"/>
    
    <!-- Exclude sensitive authentication data -->
    <exclude domain="sharedpref" path="auth_tokens.xml"/>
    <exclude domain="sharedpref" path="api_keys.xml"/>
    <exclude domain="database" path="temp_cache.db"/>
</full-backup-content>
